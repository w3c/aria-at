    "setupScript": {
      "script": "setFocusBeforeButtonAndSetStateToPressed",
      "scriptDescription": "sets focus on a link before the button, and sets the state of the button to 'pressed'",
      "name": "setFocusBeforeButtonAndSetStateToPressed",
      "source": "// sets focus on a link before the button, and sets the state of the button to 'pressed'\nlet button = testPageDocument.querySelector('#toggle');\nbutton.setAttribute('aria-pressed', 'true');\nbutton.querySelector('use').setAttribute('xlink:href', '#icon-sound');\ntestPageDocument.querySelector('#beforelink').focus();\n",
      "modulePath": "scripts/setFocusBeforeButtonAndSetStateToPressed.module.js",
      "jsonpPath": "scripts/setFocusBeforeButtonAndSetStateToPressed.jsonp.js"
    }
